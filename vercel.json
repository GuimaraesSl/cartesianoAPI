{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/jogador/(.*)", "dest": "/src/index.js" },
    { "src": "/assunto/(.*)", "dest": "/src/index.js" },
    { "src": "/problema/(.*)", "dest": "/src/index.js" },
    { "src": "/desafio/(.*)", "dest": "/src/index.js" },
    { "src": "/partida/(.*)", "dest": "/src/index.js" },
    { "src": "/armas/(.*)", "dest": "/src/index.js" },
    { "src": "/(.*)", "dest": "/src/index.js" }
  ]
}